<Application
    x:Class="NoteForYou.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="using:Microsoft.UI.Xaml.Controls"
    xmlns:local="using:NoteForYou"
    xmlns:vml="using:NoteForYou.VML">
    <Application.Resources>
        <ResourceDictionary>
            <vml:ViewModelLocator x:Key="VML" />
            <ResourceDictionary.MergedDictionaries>
                <controls:XamlControlsResources ControlsResourcesVersion="Version2" />
                <ResourceDictionary Source="ms-appx:///NoteForYou.Style/Color.xaml"/>
                <ResourceDictionary Source="ms-appx:///NoteForYou.Style/String.xaml"/>
                <ResourceDictionary Source="ms-appx:///NoteForYou.Style/Common.xaml"/>
                <ResourceDictionary Source="ms-appx:///Uwp.Core/Converter/Converter.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
